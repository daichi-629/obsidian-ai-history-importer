{
	"name": "obsidian-ai-history-importer",
	"private": true,
	"version": "0.0.1",
	"packageManager": "pnpm@9.15.0",
	"scripts": {
		"build": "pnpm -r run build",
		"dev": "pnpm --filter plugin run dev",
		"lint": "pnpm -r run lint",
		"test": "pnpm -r run test",
		"format": "prettier -w .",
		"format:check": "prettier -c .",
		"version:bump": "node ./scripts/version-bump.mjs",
		"version:patch": "pnpm run version:bump patch",
		"version:minor": "pnpm run version:bump minor",
		"version:major": "pnpm run version:bump major"
	},
	"devDependencies": {
		"@types/node": "^25.2.2",
		"esbuild": "^0.27.3",
		"eslint": "^9.39.2",
		"prettier": "^3.8.1",
		"typescript": "^5.5.4"
	}
}
